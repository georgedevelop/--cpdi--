<h2>Users</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
          <tr>
            <th>#</th>
            <th>Email</th>
            <th>Sign Up Date</th>
            <th>Is Admin?</th>
          </tr>
          </thead>
          <tbody>
          <% @users.each do |u| %>

          	<tr>
	            <td><%= u.id %></td>
	            <td><%= u.email %></td>
	            <td><%= u.created_at %></td>
	            <td><% if u.is_admin? %>Yes<% else %>No<% end %></td>
          	</tr>
          <% end %>
          </tbody>
        </table>
      </div>